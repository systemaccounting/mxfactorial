{
  "name": "mxfactorial-react",
  "version": "0.1.7",
  "private": true,
  "dependencies": {
    "@rebass/grid": "^6.0.0-4",
    "apollo-boost": "^0.1.22",
    "aws-amplify": "^1.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "crypto-js": "^3.1.9-1",
    "dayjs": "^1.7.5",
    "font-awesome": "^4.7.0",
    "graphql-tag": "^2.10.0",
    "hoist-non-react-statics": "^2.5.5",
    "jsonschema": "^1.2.4",
    "prop-types": "^15.6.1",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.3.2",
    "react-redux": "^7.0.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "latest",
    "react-svg": "^4.1.8",
    "redux": "^4.0.1",
    "rxjs": "^6.3.3",
    "styled-components": "^3.3.3",
    "styled-is": "^1.1.3",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "start": "NODE_PATH=./src react-scripts start",
    "start:env": "NODE_PATH=./src env-cmd .env.current react-scripts start && rm .env.current",
    "build:env": "NODE_PATH=./src env-cmd .env.current react-scripts build",
    "test": "react-scripts test --env=jsdom --verbose --modulePaths=./src",
    "loc:cov": "react-scripts test --forceExit --env=jsdom --modulePaths=./src --coverage",
    "test:e2e-private": "jest --detectOpenHandles --forceExit -i --config=e2e/config/private/index.js",
    "test:e2e-public": "jest --detectOpenHandles --forceExit -i --config=e2e/config/public/index.js",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{js,jsx}",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "ci:lint": "node node_modules/eslint/bin/eslint.js ./src"
  },
  "engines": {
    "node": "^11.3.0",
    "yarn": "^1.3.2"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.1",
    "coveralls": "^3.0.1",
    "cross-env": "^5.2.0",
    "env-cmd": "^8.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-jest": "^21.15.1",
    "eslint-plugin-prettier": "^2.6.0",
    "jest-puppeteer": "^4.1.1",
    "prettier": "^1.12.1",
    "puppeteer": "^1.10.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/App.js",
      "!src/setupTests.js",
      "!src/registerServiceWorker.js",
      "!src/config/**/*.js",
      "!src/mock/**/*.js",
      "!src/containers/**/*.js",
      "!src/lib/**/*.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
