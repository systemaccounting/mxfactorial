{
  "name": "rules",
  "version": "1.0.0",
  "description": "transaction automation",
  "main": "./dist/rules/src/server.js",
  "scripts": {
    "test": "jest",
    "test:single": "jest --watch --runInBand ./src/handlers/applyRules.test.ts",
    "test:integ:docker": "eval \"$(make print-docker-env-vars)\" jest --testMatch '**/src/**/*.integ.ts' ./src/tests/docker.integ.ts --testPathIgnorePatterns='[]' --silent=true ./src/tests/docker.integ.ts",
    "test:integ:cloud": "NODE_NO_WARNINGS=1 env-cmd jest --config ./src/tests/cloud.jest.config.ts --testPathIgnorePatterns='[]' --forceExit --testTimeout=60000 --silent=true --runInBand ./src/tests/cloud.integ.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "nodemon": "eval \"$(make print-docker-env-vars)\" nodemon ./src/server.ts",
    "ts-node": "eval \"$(make print-docker-env-vars)\" ts-node ./src/server.ts",
    "docker": "ts-node ./src/server.ts"
  },
  "author": "max funk",
  "license": "UNLICENSED",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.76",
    "@types/express": "^4.17.15",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/node-fetch": "^2.5.10",
    "@types/pg": "^8.6.0",
    "@urql/core": "^2.1.3",
    "axios": "^1.2.1",
    "env-cmd": "^10.1.0",
    "graphql": "^15.5.0",
    "isomorphic-unfetch": "^3.1.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "nanoid": "^3.3.3",
    "pg": "^8.6.0"
  }
}
